---
import OrnamentLeft from '@/components/OrnamentLeft.astro'
import OrnamentRight from '@/components/OrnamentRight.astro'
import RevealUp from './RevealUp.astro'
interface Props {
	label: string
}

const { label } = Astro.props
---

<RevealUp>
  <div class="flex gap-4 justify-center mb-4 md:mb-8">
    <OrnamentLeft/>
      <h3 class="font-inria text-xl md:text-2xl lg:text-4xl uppercase leading-none text-center">{ label }</h3>
    <OrnamentRight/>
  </div>
</RevealUp>